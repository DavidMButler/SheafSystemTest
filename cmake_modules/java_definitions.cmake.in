set(JAVA_FOUND @Java_FOUND@ CACHE STRING "TRUE if all JDK components are found.")
set(JAVA_EXECUTABLE @Java_JAVA_EXECUTABLE@ CACHE STRING "The full path to the Java runtime.")
set(JAR_EXECUTABLE @Java_JAR_EXECUTABLE@ CACHE STRING "The full path to the Java archiver.")
set(JAVAC_EXECUTABLE @Java_JAVAC_EXECUTABLE@ CACHE STRING "The full path to the Java compiler.")
set(JDK_BIN_DIR @Java_JDK_BIN_DIR@ CACHE STRING "The full path to the JDK bin directory.")
set(JDK_INC_DIR @Java_JDK_INC_DIR@ CACHE STRING "The full path to the JDK include directory.")
set(JDK_PLATFORM_INC_DIR @Java_JDK_PLATFORM_INC_DIR@ CACHE STRING "The full path to the JDK platform-specific include directory.")
set(JDK_LIB_DIR @Java_JDK_LIB_DIR@ CACHE STRING "The full path to the library dir.")
set(JDK_LIB_OPTIONS "-lawt -ljava -ljawt -ljvm -lmawt -lmlib_image -lverify" CACHE STRING "Linker options for the JDK.")
set(JVM_LIB_DIR @Java_JVM_LIB_DIR@ CACHE STRING "The full path to the JVM library dir.")
set(XAWT_LIB_DIR @Java_XAWT_LIB_DIR@ CACHE STRING "The full path to the XAWT library dir.")
set(JDK_LIBRARIES @Java_JDK_LIB_DIR@/libawt.so  @Java_JVM_LIB_DIR@/libjvm.so @Java_XAWT_LIB_DIR@/libzawt.so CACHE STRING "The full path to the XAWT library dir.")
set(Java_VERSION_STRING @Java_VERSION_STRING@ CACHE STRING "Version of the JDK package found (java version), eg. 1.6.0_12.")
set(Java_VERSION_MAJOR @Java_VERSION_MAJOR@ CACHE STRING "The major version of the JDK package found.")
set(Java_VERSION_MINOR @Java_VERSION_MINOR@ CACHE STRING "The minor version of the JDK package found.")
set(Java_VERSION_PATCH @Java_VERSION_PATCH@ CACHE STRING "The patch version of the JDK package found.")
set(Java_VERSION_TWEAK @Java_VERSION_TWEAK@ CACHE STRING "The tweak version of the JDK package found (after '_').")
set(Java_VERSION @Java_VERSION@ CACHE STRING "This is set to: $major.$minor.$patch(.$tweak).")
